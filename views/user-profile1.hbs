							<div class="col-md-6 mb-2"><input type="text" name="state" class="form-control"
									placeholder="Enter your state"></div>
							<div class="col-md-6 mb-2"><input name="country" type="text" class="form-control"
									placeholder="Enter your country"></div>

						</div>
						<div class="mt-5 text-center"><button class="btn btn-outline-primary profile-button" type="submit">Add
								Address</button></div>
					</form>
